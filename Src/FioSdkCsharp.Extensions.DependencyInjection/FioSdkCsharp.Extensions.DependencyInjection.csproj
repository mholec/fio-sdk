        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>FioSdk.Extensions.DependencyInjection</PackageId>
        <Version>1.1.0</Version>
        <Description>FIO Banka Client - DI Extensions</Description>
        <Copyright>Â© 2021 - 2022</Copyright>
        <Language>en-US</Language>
        <Authors>Miroslav Holec</Authors>
        <Title>FIO Banka Client - DI Extensions</Title>
        <Nullable>disable</Nullable>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageIcon>fio-sdk-3.png</PackageIcon>
